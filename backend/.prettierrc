{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "endOfLine": "lf",
    "printWidth": 80,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "tabWidth": 2
        }
      },
      {
        "files": "*.md",
        "options": {
          "proseWrap": "always"
        }
      }
    ]
  }
  